{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "data"
  ],
  "properties": {
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "id",
        "type",
        "attributes"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/data/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "1"
          ],
          "pattern": "^(.*)$"
        },
        "type": {
          "$id": "#/properties/data/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "comment"
          ],
          "pattern": "^(.*)$"
        },
        "attributes": {
          "$id": "#/properties/data/properties/attributes",
          "type": "object",
          "title": "The Attributes Schema",
          "required": [
            "body",
            "image"
          ],
          "properties": {
            "body": {
              "$id": "#/properties/data/properties/attributes/properties/body",
              "type": "string",
              "title": "The Body Schema",
              "default": "",
              "examples": [
                "comment obyd text"
              ],
              "pattern": "^(.*)$"
            },
            "image": {
              "$id": "#/properties/data/properties/attributes/properties/image",
              "type": "object",
              "title": "The Image Schema",
              "required": [
                "name",
                "record"
              ],
              "properties": {
                "name": {
                  "$id": "#/properties/data/properties/attributes/properties/image/properties/name",
                  "type": "string",
                  "title": "The Name Schema",
                  "default": "",
                  "examples": [
                    "image"
                  ],
                  "pattern": "^(.*)$"
                },
                "record": {
                  "$id": "#/properties/data/properties/attributes/properties/image/properties/record",
                  "type": "object",
                  "title": "The Record Schema",
                  "required": [
                    "id",
                    "body",
                    "task_id",
                    "created_at",
                    "updated_at"
                  ],
                  "properties": {
                    "id": {
                      "$id": "#/properties/data/properties/attributes/properties/image/properties/record/properties/id",
                      "type": "integer",
                      "title": "The Id Schema",
                      "default": 0,
                      "examples": [
                        1
                      ]
                    },
                    "body": {
                      "$id": "#/properties/data/properties/attributes/properties/image/properties/record/properties/body",
                      "type": "string",
                      "title": "The Body Schema",
                      "default": "",
                      "examples": [
                        "comment obyd text"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "task_id": {
                      "$id": "#/properties/data/properties/attributes/properties/image/properties/record/properties/task_id",
                      "type": "integer",
                      "title": "The Task_id Schema",
                      "default": 0,
                      "examples": [
                        3
                      ]
                    },
                    "created_at": {
                      "$id": "#/properties/data/properties/attributes/properties/image/properties/record/properties/created_at",
                      "type": "string",
                      "title": "The Created_at Schema",
                      "default": "",
                      "examples": [
                        "2019-08-26T09:46:57.601Z"
                      ],
                      "pattern": "^(.*)$"
                    },
                    "updated_at": {
                      "$id": "#/properties/data/properties/attributes/properties/image/properties/record/properties/updated_at",
                      "type": "string",
                      "title": "The Updated_at Schema",
                      "default": "",
                      "examples": [
                        "2019-08-26T09:46:57.601Z"
                      ],
                      "pattern": "^(.*)$"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
